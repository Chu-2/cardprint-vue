{
  "name": "cardprint-vue",
  "version": "1.0.0",
  "description": "Print cards for your Kanban board",
  "author": "Rich Yu",
  "scripts": {
    "serve": "concurrently 'yarn serve:client' 'yarn serve:lambda'",
    "serve:client": "vue-cli-service serve",
    "serve:lambda": "netlify-lambda serve src/lambda",
    "build": "yarn build:client && yarn build:lambda",
    "build:client": "vue-cli-service build",
    "build:lambda": "netlify-lambda build src/lambda",
    "lint": "vue-cli-service lint"
  },
  "main": "src/App.vue",
  "dependencies": {
    "axios": "^0.18.0",
    "core-js": "^2.6.5",
    "netlify-lambda": "^1.4.5",
    "querystringify": "^2.1.0",
    "tinycolor2": "^1.4.1",
    "vue": "^2.6.10",
    "vue-color": "^2.7.0",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.6.0",
    "@vue/cli-plugin-eslint": "^3.6.0",
    "@vue/cli-service": "^3.6.0",
    "@vue/eslint-config-prettier": "^4.0.1",
    "babel-eslint": "^10.0.1",
    "concurrently": "^4.1.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "vue-template-compiler": "^2.5.21"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/prettier"
    ],
    "rules": {
      "no-console": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "keywords": [
    "Vue",
    "kanban",
    "kanban card printer"
  ],
  "license": "MIT"
}
